<%- include header %>
    <div class="container">
      <form class="form-signin" method="post">
        <h3 class="form-signin-heading">用户注册</h3>
        <input type="text" name="phoneNumber" class="form-control" placeholder="短号(必填)" required autofocus>
        <input type="text" name="nickName" class="form-control" placeholder="昵称" required autofocus>
        <input type="password" name="password" class="form-control" placeholder="密码(必填)" required>
        <input type="password" name="rePassword" class="form-control" placeholder="确认密码(必填)" required>
        <button class="btn btn-lg btn-primary btn-block" type="submit">注册</button>
      </form>
    </div>
<%- include footer %>